<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Airline</th>
      <th>Flight<br>Number</th>
      <th></th>
    </tr>
  </thead>
    <tbody>
    <% flights.forEach(function(flight) { %>  
        <tr>
            <td><%= flight.airline %></td>
            <td><%= flight.flightNumber %></td>
            <td><a href="/flights/<%= flight._id %>">Details</a></td>
          </tr>
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>
